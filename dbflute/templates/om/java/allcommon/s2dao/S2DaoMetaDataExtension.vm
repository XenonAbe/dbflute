    protected ${glInternalUpdateBatchAutoStaticCommand} createInternalUpdateBatchAutoStaticCommand(final Method method, final String[] propertyNames, boolean returningRows) {
        return new ${glInternalUpdateBatchAutoStaticCommand}(dataSource, statementFactory, createBeanMetaData4UpdateDeleteByAuto(method), propertyNames, returningRows) {
		    @Override
            public Object execute(Object[] args) {
                final Object result = super.execute(args);
                handleBatchUpdateResultWithOptimisticLock(args, result);
                return result;
            }
        };
    }

    protected void handleBatchUpdateResultWithOptimisticLock(Object[] args, Object result) {
        if (args.length == 0) {
            return;// for Safety!
        }
        if (result instanceof int[]) {
            final int[] updatedCountArray = (int[])result;
            int index = 0;
            for (int updatedCount : updatedCountArray) {
                if (updatedCount == 0) {
                    if (args.length <= index) {
                        return;// for Safety!
                    }
                    throw new ${glPackageBaseCommonException}.${glEntityAlreadyUpdatedException}(args[index], updatedCount);
                }
            }
        }
    }

