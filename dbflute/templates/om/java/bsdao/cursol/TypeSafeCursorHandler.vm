${database.allClassCopyright}package ${myBaseDaoCursorPackageName};

#set ($myClassName = "${myBaseTypeSafeCursorHandlerClassName}")

import ${myExtendedDaoCursorPackageName}.${myExtendedTypeSafeCursorClassName};

/**
 * The cursor handler of ${table.name}.
 * 
 * @author ${database.ClassAuthor}
 */${database.outputSuppressWarningsAfterLineSeparator()}
public abstract class ${myClassName} implements ${glCursorHandlerName} {

    /**
     * Handle.
     * 
     * @param rs Result set. (NotNull)
     * @return Result. (Nullable)
     */
    public Object handle(java.sql.ResultSet rs) {
        return fetchCursor(createTypeSafeCursor(rs));
    }

    /**
     * Create type safe cursor.
     * 
     * @param rs Result set. (NotNull)
     * @return Type safe cursor. (Nullable)
     */
    protected ${myExtendedTypeSafeCursorClassName} createTypeSafeCursor(java.sql.ResultSet rs) {
        return new ${myExtendedTypeSafeCursorClassName}(rs);
    }

    /**
     * Fetch cursor.
     * 
     * @param rs Type safe cursor. (NotNull)
     * @return Result. (Nullable)
     */
    abstract protected Object fetchCursor(${myTypeSafeResultSetClassName} rs);
}
