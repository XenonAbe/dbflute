${database.allClassCopyright}package ${myBaseDaoCursorPackageName};

#set ($myClassName = "${myTypeSafeCursorClassName}")

/**
 * The cursor of ${table.name}.
 * 
 * @author ${database.ClassAuthor}
 */${database.outputSuppressWarningsAfterLineSeparator()}
public class ${myClassName} {

    /** Wrapped result set. */
    protected java.sql.ResultSet _rs;

    /**
     * Constructor.
     * 
     * @param rs Wrapped result set. (NotNull)
     */
    public ${myClassName}(java.sql.ResultSet rs) {
        this._rs = rs;
    }

    /**
     * Get wrapped result set.
     * 
     * @return Wrapped result set. (NotNull)
     */
    public java.sql.ResultSet cursor() {
        return _rs;
    }

#foreach ($col in $table.columns)
    public ${col.javaNative} get${col.javaName}() {
        return (${col.javaNative})extractValueAsObject("${col.name}");
    }
#end

    protected Object extractValueAsObject(String name) {
        try {
            return _rs.getObject(name);
        } catch (java.sql.SQLException e) {
            String msg = "java.sql.ResultSet#getObject() threw the SQLException; columnName=${col.name}";
            throw new RuntimeException(msg);
        }
    }

    protected void handleSQLException(java.sql.SQLException e) {
        String msg = "java.sql.ResultSet#getObject() threw the SQLException; columnName=${col.name}";
        throw new RuntimeException(msg);
    }
}
