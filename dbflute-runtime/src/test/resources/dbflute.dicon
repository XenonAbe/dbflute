<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR2.1//DTD S2Container//EN"
    "http://www.seasar.org/dtd/components21.dtd">

<components namespace="dbflute">
    <include path="j2ee.dicon"/>

    <!-- The client component of DBFlute. (Basically internal components except for behaviorSelector) -->
    <component class="com.example.dbflute.basic.dbflute.allcommon.DBFluteInitializer"/>
    <component name="invokerAssistant" class="com.example.dbflute.basic.dbflute.allcommon.ImplementedInvokerAssistant"/>
    <component name="commonColumnAutoSetupper" class="com.example.dbflute.basic.dbflute.allcommon.ImplementedCommonColumnAutoSetupper"/>
    <component name="behaviorSelector" class="com.example.dbflute.basic.dbflute.allcommon.ImplementedBehaviorSelector">
        <destroyMethod name="destroy"/>
    </component>

    <!-- The runtime component of DBFlute. -->
    <component name="behaviorCommandInvoker" class="org.dbflute.bhv.core.BehaviorCommandInvoker"/>
    <component name="beanMetaDataFactory" class="org.dbflute.s2dao.extension.TnBeanMetaDataFactoryExtension"/>
    <component name="valueTypeFactory" class="org.dbflute.s2dao.valuetype.impl.TnValueTypeFactoryImpl"/>

    <component name="memberBhv" class="com.example.dbflute.basic.dbflute.exbhv.MemberBhv"/>
    <component name="memberAddressBhv" class="com.example.dbflute.basic.dbflute.exbhv.MemberAddressBhv"/>
    <component name="memberLoginBhv" class="com.example.dbflute.basic.dbflute.exbhv.MemberLoginBhv"/>
    <component name="memberSecurityBhv" class="com.example.dbflute.basic.dbflute.exbhv.MemberSecurityBhv"/>
    <component name="memberStatusBhv" class="com.example.dbflute.basic.dbflute.exbhv.MemberStatusBhv"/>
    <component name="memberWithdrawalBhv" class="com.example.dbflute.basic.dbflute.exbhv.MemberWithdrawalBhv"/>
    <component name="productBhv" class="com.example.dbflute.basic.dbflute.exbhv.ProductBhv"/>
    <component name="productStatusBhv" class="com.example.dbflute.basic.dbflute.exbhv.ProductStatusBhv"/>
    <component name="purchaseBhv" class="com.example.dbflute.basic.dbflute.exbhv.PurchaseBhv"/>
    <component name="vendorCheckBhv" class="com.example.dbflute.basic.dbflute.exbhv.VendorCheckBhv"/>
    <component name="withdrawalReasonBhv" class="com.example.dbflute.basic.dbflute.exbhv.WithdrawalReasonBhv"/>
    <component name="summaryProductBhv" class="com.example.dbflute.basic.dbflute.exbhv.SummaryProductBhv"/>

</components>
